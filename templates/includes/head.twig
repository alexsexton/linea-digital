<head>
  <meta charset="utf-8">

  <title>{% if  entry.title is defined %}{{ entry.title }} | {% elseif title is defined %}{{ title }} | {% endif %} {{ siteName }}</title>

  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="canonical" href="{{ entry.url }}">
  {% if siteDescription.metaDescription is defined %}
  <meta name="description" content="{{ siteDescription.metaDescription }}">
  {% endif %}
  <link rel="dns-prefetch" href="//use.typekit.net">
  {% if devMode %}
  <link rel="stylesheet" href="{{ siteUrl }}static/main.css?v={{ "now"|date("sf") }}" media="screen">
  {% else %}
  <link rel="stylesheet" href="{{ siteUrl }}static/main.css?v={{ "now"|date("HdI") }}" media="screen">
  {% endif %}

  <link rel="stylesheet" href="https://use.typekit.net/gny0vgl.css">

  <meta property="og:type" content="website">
  {% if entry.title is defined %}
  <meta property="og:title" content="{{ entry.title }}">
  <meta property="og:url" content="{{ entry.url }}">
  {% endif %}
  {% if siteDescription.metaDescription is defined %}
  <meta property="og:description" content="{{ siteDescription.metaDescription }}">
  {% endif %}
  <meta property="og:site_name" content="{{ siteName }}">

  <link rel="apple-touch-icon" href="{{ siteUrl }}static/images/apple-touch-icon.png">
  <link rel="icon" type="image/png" href="{{ siteUrl }}static/images/favicon.png">

</head>
